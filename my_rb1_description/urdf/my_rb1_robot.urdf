<?xml version="1.0" ?>

<robot name="RB1">

    <!-- Link - base_footprint -->
    <link name="base_footprint">
    </link>

    <!-- Joint - base_footprint / base_link -->
    <joint name="joint_base_footprint_base_link" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link" />
        <origin xyz="0 0 0.15" rpy="0 0 0"/>
    </joint>

    <!-- Link - base_link -->
    <link name="base_link">
        <inertial>
            <mass value="20"/>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.23125" ixy="0" ixz="0" iyy="0.23125" iyz="0" izz="0.3125" />
        </inertial>

        <collision>
            <geometry>
                <cylinder radius="0.25" length="0.3"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius="0.25" length="0.3"/>
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - base_link / right_wheel -->
    <joint name="joint_base_link_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
        <child link="right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - right_wheel -->
    <link name="right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00765625" ixy="0" ixz="0" iyy="0.00765625" iyz="0" izz="0.0003125" />
        </inertial>

        <collision>
            <origin rpy="1.57 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.025"/>
            </geometry>
        </collision>

        <visual>
            <origin rpy="1.57 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - base_link / left_wheel -->
    <joint name="joint_base_link_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.2 -0.15" />
        <child link="left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - left_wheel -->
    <link name="left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00765625" ixy="0" ixz="0" iyy="0.00765625" iyz="0" izz="0.0003125" />
        </inertial>

        <collision>
            <origin rpy="1.57 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.025"/>
            </geometry>
        </collision>

        <visual>
            <origin rpy="1.57 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - base_link / front_caster -->
    <joint name="joint_base_link_right_front_caster" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0 -0.15" />
        <child link="front_caster" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0" />
    </joint>

    <!-- Link - front_caster -->
    <link name="front_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <!-- Hollow sphere of radius r and mass m -->
            <inertia ixx="0.000416" ixy="0" ixz="0" iyy="0.000416" iyz="0" izz="0.000416"/>
        </inertial>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <!--collision>
            <origin rpy="0 0 0" xyz="0 0 -0.015" />
            <geometry>
                <sphere radius="0.0125" />
            </geometry>
        </collision-->
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.015" />
            <geometry>
                <sphere radius="0.0125" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>

    </link>

    <!-- Joint - base_link / back_caster -->
    <joint name="joint_base_link_back_caster" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0 -0.15" />
        <child link="back_caster" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0" />
    </joint>

    <!-- Link - back_caster -->
    <link name="back_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <!-- Hollow sphere of radius r and mass m -->
            <inertia ixx="0.000416" ixy="0" ixz="0" iyy="0.000416" iyz="0" izz="0.000416"/>
        </inertial>

        <!-- Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <!--collision>
            <origin rpy="0 0 0" xyz="0 0 -0.015" />
            <geometry>
                <sphere radius="0.0125" />
            </geometry>
        </collision-->
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.015" />
            <geometry>
                <sphere radius="0.0125" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>

    </link>

    <!-- Joint - base_link / front_laser -->
    <joint name="joint_base_link_front_laser" type="fixed">
        <origin rpy="3.14 0 0" xyz="0.25 0.0 0.075" />
        <child link="front_laser" />
        <parent link="base_link" />
    </joint>

    <!-- Link - front_laser -->
    <link name="front_laser">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.000252666666667" ixy="0" ixz="0" iyy="0.000252666666667" iyz="0" izz="0.0005"/>
        </inertial>
        <!-- visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.15" length="0.20"/>
            </geometry>
            <material name="Red">
                <color rgba="0.7 0.1 0.1 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.15" length="0.20"/>
            </geometry>
        </collision-->
    </link>

    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>

</robot>